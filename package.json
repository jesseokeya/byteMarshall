{
  "name": "bytemarshall",
  "version": "1.0.0",
  "description": "A run time environment for various programming languages",
  "main": "server.js",
  "scripts": {
    "test": "mocha ./test/**/**/*.test.js",
    "server": "nodemon server.js",
    "client": "cd client && npm start && cd ..",
    "start": "npm run server",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "deploy": "cd client; npm run build; cd ..; git add .; git commit -m 'changes'; git push"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jesseokeya/byteMarshall.git"
  },
  "keywords": [
    "nodejs",
    "linux",
    "go",
    "python",
    "node"
  ],
  "author": "Jesse Okeya",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jesseokeya/byteMarshall/issues"
  },
  "homepage": "https://github.com/jesseokeya/byteMarshall#readme",
  "dependencies": {
    "concurrently": "^4.1.2",
    "dotenv": "^6.2.0",
    "hasbin": "^1.2.3",
    "jsonwebtoken": "^9.0.0",
    "jwt-decode": "^2.2.0",
    "koa": "^2.11.0",
    "koa-bodyparser": "^4.2.1",
    "koa-json": "^2.0.2",
    "koa-logger": "^3.2.1",
    "koa-router": "^7.4.0",
    "koa-send": "^5.0.0",
    "koa-socket-2": "^1.2.0",
    "koa-static": "^5.0.0",
    "koa2-cors": "^2.0.6",
    "lodash": "^4.17.15",
    "mongoose": "^5.8.1",
    "nodemon": "^1.19.4",
    "shelljs": "^0.8.3",
    "write": "^2.0.0"
  },
  "devDependencies": {
    "mocha": "^5.2.0"
  }
}
